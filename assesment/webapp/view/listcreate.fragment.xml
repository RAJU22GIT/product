<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout">
<Dialog title="Create List">
    <f:SimpleForm>
        <f:content>
            <Label text="vendor Id"/>
            <Input value="{listtwo>/vendorId}"/>
            <Label text="vendor Name"/>
            <Input value="{listtwo>/vendorName}"/>
        </f:content>
    </f:SimpleForm>
    <beginButton>
        <Button text="Save" press="onSaveProduct"/>
    </beginButton>
    <endButton>
    <Button text="Cancel" press="onCancelProduct"/>
    </endButton>
</Dialog>
</core:FragmentDefinition>